    <value key="enabled">True</value>
    <value key="sortable">True</value>
    <value key="operations">
        <element>plone.app.querystring.operation.selection.is</element>
    </value>
    <value key="vocabulary">collective.abcmusic.vocabularies.abctunetypes</value>
    <!-- see /plone/app/vocabularies/... -->
    <value key="group" i18n:translate="">Text</value>
  </records>
  <records interface="collective.js.jqueryui.controlpanel.IJQueryUICSS">
      <value key="css">False</value>
      <value key="patch">False</value>
    </records>

</registry>