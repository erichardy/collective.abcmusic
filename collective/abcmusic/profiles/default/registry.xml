    i18n:domain="collective.abcmusic"
>
  <records interface="collective.abcmusic.controlpanel.IABCMusicSettingsForm" />
  
  <records interface="plone.app.querystring.interfaces.IQueryField"
           prefix="plone.app.querystring.field.abcTuneType">
